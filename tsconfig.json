{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": false,
    "inlineSourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "typedocOptions": {
    "entryPoints": [
      "lib/**/*.ts",
      "app/**/*.tsx",
      "components/**/*.tsx",
      "declarations/**/*.ts"
    ],
    "out": "docs",
    "excludePrivate": false
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "app/herstelleralt/threedotsschlug/page.tsxxxxx", ".tmp/mdx.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}